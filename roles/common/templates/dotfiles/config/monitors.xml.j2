<monitors version="1">
  <configuration>
      <clone>no</clone>
      <output name="VGA-1">
          <vendor>VBX</vendor>
          <product>0x0000</product>
          <serial>0x03f60780</serial>
          <width>{{ monitor_width }}</width>
          <height>{{ monitor_height }}</height>
          <rate>60</rate>
          <x>0</x>
          <y>0</y>
          <rotation>normal</rotation>
          <reflect_x>no</reflect_x>
          <reflect_y>no</reflect_y>
          <primary>yes</primary>
      </output>
  </configuration>
</monitors>

